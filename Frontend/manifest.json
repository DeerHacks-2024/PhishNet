{
  "manifest_version": 3,
  "name": "catPhish",
  "description": "Detect whether a page is possibly phishing or not.",
  "version": "1.0.0",
  "permissions": ["webNavigation", "activeTab", "storage"],
  "action": {
    "default_popup": "js/index.html",
    "default_title": "catPhish"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}
